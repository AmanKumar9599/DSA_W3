import java.util.*;
public class Main{
     public static void fun(int n){
          int a=n%3;
          if(a==0){
               System.out.println("Second");
          }
          else
          System.out.println("First");
          
     }
     public static void main(String args[]){
          Scanner sc = new Scanner(System.in);
         int t=sc.nextInt();
         while(t-->0){
              int n=sc.nextInt();
              fun(n);
         }
     }
}

