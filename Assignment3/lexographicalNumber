class Solution {
    public List<Integer> lexicalOrder(int n) {
        List<Integer> list = new ArrayList<>();
        
        for(int i=1;i<10;i++){
            print(n,i,list);
        }

        return list;
    }
    public void print(int n,int ans,List<Integer> list){
        if(ans<=n){
            list.add(ans);
        }
        if(ans>n) return;

        for(int i=0;i<10;i++){
            print(n,ans*10+i,list);
        }
    }
}
