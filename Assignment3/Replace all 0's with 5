class Solution {
    int convertfive(int num) {
        // Your code here
        String s =Integer.toString(num);
        StringBuilder ans=new StringBuilder();
        for(int i=0;i<s.length();i++){
            char ch=s.charAt(i);
            if(ch=='0'){
                ans.append('5');
            }
            else
            ans.append(ch);
        }
        return Integer.parseInt(ans.toString());
    }
}
