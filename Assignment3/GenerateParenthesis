class Solution {
    public void gen(ArrayList<String> st,String ans, int op,int cl,int n){
        if(op==cl && op+cl==n*2){
            st.add(ans);
            return;
        }
        if(op<n)
        gen(st,ans+'(',op+1,cl,n);
        if(cl<op)
        gen(st,ans+')',op,cl+1,n);
    }
    public List<String> generateParenthesis(int n) {
         ArrayList<String> st = new ArrayList<>();
        gen(st,"",0,0,n);
        return st;
    }
}
