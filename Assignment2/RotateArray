class Solution {
    public static void fun(int []arr,int i,int j){
        while(i<=j){
            int temp=arr[i];
            arr[i]=arr[j];
            arr[j]=temp;
            i++;
            j--;
        }
    }
    static void rotateArr(int arr[], int d, int n) {
        // add your code here
        d%=n;
        fun(arr,0,n-1);
        fun(arr,0,n-d-1);
        fun(arr,n-d,n-1);
    }
}
