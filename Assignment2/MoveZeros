class Solution {
    public static int[] swap(int[]arr,int i,int j){
        int temp=arr[i];
		arr[i]=arr[j];
		arr[j]=temp;
		
		return arr;
    }
    public void moveZeroes(int[] arr) {
        int n=arr.length;
        int i=0;
        int j=n-1;
        while(i<j){
            if(arr[i]!=0){
                i++;
            }
            if(arr[j]==0){
                j--;
            }
            if(arr[j]!=0){
                swap(arr,i,j);
            }
        }
}
}
